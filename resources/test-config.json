{
    "db-spec":
    {
        "classname":"org.postgresql.Driver",
        "subprotocol":"postgresql",
        "host":"{{DB_HOST}}",
        "port":"{{DB_PORT}}",
        "subname":"{{DB_NAME}}",
        "user":"postgres",
        "password":"test",
        "max-conns":6,
        "min-conns":1,
        "init-conns":1
    },
    "secret":"resources/vape.test.key",
    "thread-util":0.9,
    "max-threads":64,
    "server":
    {
        "port":8080,
        "join?":false
    },
    "nsq":{
        "lookup":{"host":"{{NSQ_LOOKUP_HOST}}","port":"{{NSQ_LOOKUP_PORT}}"},
        "produce":{"host":"{{NSQ_PRODUCE_HOST}}","port":"{{NSQ_PRODUCE_PORT}}"}
    }
}
